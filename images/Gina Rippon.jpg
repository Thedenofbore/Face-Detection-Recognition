	function searchResult() {
            var searchByCategories = false;

        var urlResult;
        var searchAttribute;
        var inputValue = $('#searchText').val();
        var selectValue = $('#estudiSelect').val();

        if (searchByCategories) {
            if (selectValue) {
                searchAttribute = selectValue;

            } else {
                searchAttribute = '?q=';
            }
            urlResult = '';
        } else {
            searchAttribute = '?q=';
            urlResult = selectValue;
        }

        if (searchByCategories && selectValue && inputValue) {
            inputValue = '&q=' + inputValue;
        }

        if (urlResult) {
            if(inputValue != ''){
                checkFormToGoResultsPage(urlResult + searchAttribute + inputValue);
            } else {
				if (searchByCategories) {
                    checkFormToGoResultsPage(urlResult + searchAttribute);
				}else {
					checkFormToGoResultsPage(urlResult);
				}
            }
        } else {
            $('#_com_liferay_journal_content_web_portlet_JournalContentPortlet_INSTANCE_FWCCAXSIIgxo_fm')[0].reportValidity();
        }

        function checkFormToGoResultsPage(url) {
            if($('#_com_liferay_journal_content_web_portlet_JournalContentPortlet_INSTANCE_FWCCAXSIIgxo_fm')[0].checkValidity()) {
                location.href = url;
            } else {
                $('#_com_liferay_journal_content_web_portlet_JournalContentPortlet_INSTANCE_FWCCAXSIIgxo_fm')[0].reportValidity();
            }
        }
	}
	document.getElementById("botonBusqueda").addEventListener('click',function () {
