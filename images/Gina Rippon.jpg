<script>
    var propuestas = ['Arxiu històric','Beques i ajuts','Cultura','Divulgació','Doctorat','Facultats','Graus','Igualtat','Màsters i postgraus','Participa','Producció científica','Responsabilitat social','Rànquings','Serveis científics','Transparència',];
    var length = propuestas.length;
    var i = 0;
    var x = 0;
    var speed = 100;
    var texto = "";

    var placeHolder = {
    	dzjx__modify: function (){
    			if (i < length){
    				document.getElementById("dzjx__searchField").placeholder = "";
    				let timerId = window.setTimeout(typeWriter.dzjx__write, speed);
    				i++;
    			} else {
    				i = 1;
    				document.getElementById("dzjx__searchField").placeholder = "";
    				timerId = window.setTimeout(typeWriter.dzjx__write, speed);
    			}
    	}
	}

    var typeWriter = {
    	dzjx__write: function () {
    		if (x < propuestas[i-1].length) {
    			document.getElementById("dzjx__searchField").placeholder += propuestas[i-1].charAt(x);
    			x++;
    			timerId = setTimeout(typeWriter.dzjx__write, speed);
    		} else {
    			x = 0;
    		}
    	}
	}

    let intervalId = setInterval(placeHolder.dzjx__modify,2000);

    document.getElementById("dzjx__searchField").onblur = function() {
		texto = document.getElementById("dzjx__searchField").value;
		if(texto == ""){
			intervalId = setInterval(placeHolder.dzjx__modify,2000);
		} else {
			document.getElementById("dzjx__searchField").placeholder = "";
		}
	};

    document.getElementById("dzjx__searchField").onfocus = function() {
		clearTimeout(timerId);
		clearInterval(intervalId);
		x = 0;
		texto = document.getElementById("dzjx__searchField").value;
		if(texto == ""){
			document.getElementById("dzjx__searchField").placeholder = "Cerca per text";
		} else {
			document.getElementById("dzjx__searchField").placeholder = "";
		}
	};
	
    document.addEventListener('visibilitychange', function() {
		if(document.hidden) {
			clearTimeout(timerId);
			clearInterval(intervalId);
			x = 0;
			texto = document.getElementById("dzjx__searchField").value;
			if(texto == "") {
				document.getElementById("dzjx__searchField").placeholder = "Cerca per text";
			} else {
				document.getElementById("dzjx__searchField").placeholder = "";
			}
		} else {
			texto = document.getElementById("dzjx__searchField").value;
			if(texto == "") {
				intervalId = setInterval(placeHolder.dzjx__modify,2000);
			} else {
				document.getElementById("dzjx__searchField").placeholder = "";
			}
		}
	});
</script>
